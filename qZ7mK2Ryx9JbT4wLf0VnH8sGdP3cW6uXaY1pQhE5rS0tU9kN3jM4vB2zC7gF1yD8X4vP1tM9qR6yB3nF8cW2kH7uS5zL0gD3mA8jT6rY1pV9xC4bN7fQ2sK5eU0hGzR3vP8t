local ESPMode = false
local ClearESP = nil

local function clearESP()
    local Timer = 0
    ClearESP = RunService.Heartbeat:Connect(function(dt)
	    Timer += dt
	    if Timer >= 1 then
            if not ESPMode then
                for i, c in pairs(COREGUI:GetChildren()) do
		            if string.sub(c.Name, -4) == '_ESP' then
			            c:Destroy()
		            end
            	end
            end
            Timer -= 1
	    end
    end)
end

clearESP()

local function ESPStroke(color)
    local lum = 0.299 * color.R + 0.587 * color.G + 0.114 * color.B
    return lum < 0.5 
end

local function HPColor(hum, health)
	local hp = hum.Health / hum.MaxHealth
	if hp > 0.6 then
		health.TextColor3 = Green
	elseif hp > 0.3 then
		health.TextColor3 = Yellow
	else
		health.TextColor3 = Red
	end
end

local function ESP(plr, logic)
	task.spawn(function()
		for i,v in pairs(COREGUI:GetChildren()) do
			if v.Name == plr.Name..'_ESP' then
				v:Destroy()
			end
		end
		wait()
		if plr.Character and plr.Name ~= Players.LocalPlayer.Name and not COREGUI:FindFirstChild(plr.Name..'_ESP') then
			local ESPholder = Instance.new("Folder")
			ESPholder.Name = plr.Name..'_ESP'
			ESPholder.Parent = COREGUI
			repeat wait(1) until plr.Character and getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid")
			for b,n in pairs (plr.Character:GetChildren()) do
				if (n:IsA("BasePart")) then
					local a = Instance.new("BoxHandleAdornment")
					a.Name = plr.Name
					a.Parent = ESPholder
					a.Adornee = n
					a.AlwaysOnTop = true
					a.ZIndex = 10
					a.Size = n.Size
					a.Transparency = espTransparency
					if logic == true then
						a.Color = BrickColor.new(plr.TeamColor == Players.LocalPlayer.TeamColor and "Bright green" or "Bright red")
					else
						a.Color = plr.TeamColor
					end
				end
			end
			if plr.Character and plr.Character:FindFirstChild('Head') then
				local ESPGui = Instance.new("BillboardGui")
				ESPGui.Name = plr.Name
				ESPGui.Size = UDim2.new(0, 100, 0, 150)
				ESPGui.StudsOffset = Vector3.new(0, 1, 0)
				ESPGui.AlwaysOnTop = true
                ESPGui.Adornee = plr.Character.Head
                ESPGui.Parent = ESPholder
                local ESPStuds = Instance.new("TextLabel")
				ESPStuds.BackgroundTransparency = 1
                ESPStuds.Size = UDim2.new(0, 100, 0, 100)
				ESPStuds.Position = UDim2.new(0, 0, 0, -80)
				ESPStuds.Font = Enum.Font.SourceSansItalic
				ESPStuds.TextSize = 15
				ESPStuds.TextColor3 = White
                ESPStuds.TextStrokeTransparency = 0
				ESPStuds.TextYAlignment = Enum.TextYAlignment.Bottom
				ESPStuds.Text = plr.Name
				ESPStuds.ZIndex = 10
                ESPStuds.Parent = ESPGui
                local ESPName = Instance.new("TextLabel")
				ESPName.BackgroundTransparency = 1
                ESPName.Size = UDim2.new(0, 100, 0, 100)
				ESPName.Position = UDim2.new(0, 0, 0, -65)
				ESPName.Font = Enum.Font.Bangers
				ESPName.TextSize = 15
				ESPName.TextColor3 = plr.TeamColor.Color
				ESPName.TextStrokeTransparency = 0
				ESPName.TextYAlignment = Enum.TextYAlignment.Bottom
                ESPName.TextStrokeColor3 = ESPStroke(ESPName.TextColor3) and Color3.new(1,1,1) or Color3.new(0,0,0)
				ESPName.ZIndex = 10
                ESPName.Parent = ESPGui
                local ESPHealth = Instance.new("TextLabel")
				ESPHealth.BackgroundTransparency = 1
                ESPHealth.Size = UDim2.new(0, 100, 0, 100)
				ESPHealth.Position = UDim2.new(0, 0, 0, -50)
				ESPHealth.Font = Enum.Font.Bangers
				ESPHealth.TextSize = 15
				ESPHealth.TextColor3 = Green
                ESPHealth.TextStrokeTransparency = 0
				ESPHealth.TextYAlignment = Enum.TextYAlignment.Bottom
				ESPHealth.Text = plr.Name
				ESPHealth.ZIndex = 10
                ESPHealth.Parent = ESPGui
				local espLoopFunc
				local teamChange
				local addedFunc
				addedFunc = plr.CharacterAdded:Connect(function()
					if ESPMode then
						espLoopFunc:Disconnect()
						teamChange:Disconnect()
						ESPholder:Destroy()
						repeat wait(1) until getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid")
						ESP(plr, logic)
						addedFunc:Disconnect()
					else
						teamChange:Disconnect()
						addedFunc:Disconnect()
					end
				end)
				teamChange = plr:GetPropertyChangedSignal("TeamColor"):Connect(function()
					if ESPMode then
						espLoopFunc:Disconnect()
						addedFunc:Disconnect()
						ESPholder:Destroy()
						repeat wait(1) until getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid")
						ESP(plr, logic)
						teamChange:Disconnect()
					else
						teamChange:Disconnect()
					end
				end)
				local function espLoop()
					if COREGUI:FindFirstChild(plr.Name..'_ESP') then
						if plr.Character and getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid") and Players.LocalPlayer.Character and getRoot(Players.LocalPlayer.Character) and Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
                            ESPStuds.Text = math.floor((getRoot(Players.LocalPlayer.Character).Position - getRoot(plr.Character).Position).magnitude)
						    ESPName.Text = plr.Name
                            local humanoid = plr.Character:FindFirstChildOfClass('Humanoid')
                            if humanoid then
	                            ESPHealth.Text = math.floor(humanoid.Health)
	                            HPColor(humanoid, ESPHealth)
                            end
						end
					else
						teamChange:Disconnect()
						addedFunc:Disconnect()
						espLoopFunc:Disconnect()
					end
				end
				espLoopFunc = RunService.RenderStepped:Connect(espLoop)
			end
		end
	end)
end

TextButton12.Activated:Connect(function()
    if Lock7 then
        return
    end
    OnOff8 = not OnOff8
    Frame4.BackgroundColor3 = OnOff8 and White or Black
    if OnOff8 then
		ESPMode = true
		for i, v in pairs(Players:GetPlayers()) do
			if v.Name ~= speaker.Name then
				ESP(v)
		 	end
	    end
    else
        ESPMode = false
    	for i, c in pairs(COREGUI:GetChildren()) do
		    if string.sub(c.Name, -4) == '_ESP' then
			    c:Destroy()
		    end
    	end
        Lock7 = true
        task.wait(1)
        Lock7 = false
    end
end)

local InvisVoid = nil
local LoopInvis = nil

local function InvisTransparency(num)
    if myCharacter then 
        for _, Part in ipairs(myCharacter:GetDescendants()) do
            if (Part:IsA("BasePart") and Part.Name ~= "HumanoidRootPart") or Part:IsA("Decal") then
                if Part.Transparency ~= 1 then
                    Part.Transparency = num
                end
            end
        end
    end
end

TextButton11.Activated:Connect(function()
    if Lock6 then
        return
    end
    OnOff7 = not OnOff7
    Frame3.BackgroundColor3 = OnOff7 and White or Black
    if OnOff7 then
        Lock6 = true
        if myRootPart then
            if workspace and workspace:FindFirstChild("InvisChair") then
                workspace.InvisChair:Destroy()
            end
            InvisVoid = workspace and workspace.FallenPartsDestroyHeight or -500
            local OldInvis = myRootPart.CFrame
            workspace.FallenPartsDestroyHeight = 0/0
            if LoopInvis then
                LoopInvis:Disconnect()
                LoopInvis = nil
            end
            local Timer = 0
            LoopInvis = RunService.Heartbeat:Connect(function(dt)
	            Timer += dt
	            if Timer >= 0.1 then
		            InvisTransparency(0.5)
		            execCmd("nosit")
	            	Timer -= 0.1
	            end
            end)
            if myHumanoid and myHumanoid.SeatPart then
				execCmd("jump")
                task.wait(0.2)
            end
            task.wait(0.1)
            myRootPart.CFrame = CFrame.new(0, -1/0, 0)
            task.wait(0.15)
            local InvisSeat = Instance.new("Seat")
            InvisSeat.Name = "InvisChair"
            InvisSeat.Anchored = false
            InvisSeat.CanCollide = false
            InvisSeat.Transparency = 1
            InvisSeat.Position = Vector3.new(0, -1/0, 0)
            InvisSeat.Parent = workspace
            local InvisWeld = Instance.new("Weld")
            InvisWeld.Part0 = InvisSeat
            InvisWeld.Part1 = myRootPart
            InvisWeld.Parent = InvisSeat
            InvisSeat.CFrame = OldInvis 
            OldInvis = nil
        end
        Lock6 = false
    else
        if workspace and workspace:FindFirstChild("InvisChair") then
            workspace.InvisChair:Destroy()
        end
        if workspace and InvisVoid then
            workspace.FallenPartsDestroyHeight = InvisVoid
        end
        if LoopInvis then
            LoopInvis:Disconnect()
            LoopInvis = nil
        end
        InvisTransparency(0)
        Lock6 = true
        task.wait(1)
        execCmd('unnosit')
        Lock6 = false
    end
end)

local TouchFling = false

TextButton9.Activated:Connect(function()
    if Lock5 then
        return
    end
    OnOff6 = not OnOff6
    TextButton9.Text = OnOff6 and "Touch Fling / On" or "Touch Fling / Off"
    if OnOff6 then
        OnDied()
        TouchFling = false
        task.wait()
        TouchFling = true
        repeat RunService.Heartbeat:Wait()
        local power = 10000
        if TextBox6.Text and isNumber(TextBox6.Text) then
            power = tonumber(TextBox6.Text)
        end
        local character = speaker.Character
        local root = getRoot(character)
        local vel, movel = nil, 0.1
        while not (character and character.Parent and root and root.Parent) do
            RunService.Heartbeat:Wait()
            character = speaker.Character
            root = getRoot(character)
        end
        vel = root.Velocity
        root.Velocity = vel * power + Vector3.new(0, power, 0)
        RunService.RenderStepped:Wait()
        if character and character.Parent and root and root.Parent then
            root.Velocity = vel
        end
        RunService.Stepped:Wait()
        if character and character.Parent and root and root.Parent then
            root.Velocity = vel + Vector3.new(0, movel, 0)
                movel = movel * -1
            end
        until not TouchFling
    else
        TouchFling = false
        Lock5 = true
        task.wait(1)
        Lock5 = false
    end
end)

AntiFling = nil

TextButton8.Activated:Connect(function()
    if Lock4 then
        return
    end
    OnOff5 = not OnOff5
    Frame2.BackgroundColor3 = OnOff5 and White or Black
    if OnOff5 then
        if AntiFling then
            AntiFling:Disconnect()
            AntiFling = nil
        end
        AntiFling = RunService.Stepped:Connect(function()
            for _, p in pairs(Players:GetPlayers()) do
                if p ~= speaker and p.Character then
                    for _, v in pairs(p.Character:GetDescendants()) do
                        if v:IsA("BasePart") then
                            v.CanCollide = false
                        end
                    end
                end
            end
        end)
    else
        if AntiFling then
            AntiFling:Disconnect()
            AntiFling = nil
        end
        Lock4 = true
        task.wait(1)
        Lock4 = false
    end
end)

local FlingLock = false
local SkidFling_ = false
local FlingDied = nil
local OldFling = nil
local OldFling_ = nil
local FlingVel = nil

local function SkidFling(vFling)
    if FlingLock then
        return
    end
    if Lock3 then
        return
    end
    if TextBox5.Text:match("^%s*$") or TextBox5.Text == "me" then
        return
    end
    local players_ = getPlayer(TextBox5.Text, speaker)
    if #players_ == 0 then
        return
    end
    Frame1.BackgroundColor3 = White
    Lock3 = true
    SkidFling_ = false
    task.wait()
    SkidFling_ = true
    if FlingDied then
        FlingDied:Disconnect()
        FlingDied = nil
    end
    local character = speaker.Character
    local Humanoid = character and character:FindFirstChildWhichIsA("Humanoid") 
    local RootPart = character and getRoot(character)
    local TCharacter = vFling.Character
    local THumanoid = TCharacter and TCharacter:FindFirstChildWhichIsA("Humanoid") 
    local TRootPart = TCharacter and getRoot(TCharacter)
    local THead = TCharacter and TCharacter:FindFirstChild("Head") 
    local Accessory = TCharacter and TCharacter:FindFirstChildWhichIsA("Accessory")
    local Handle = Accessory and Accessory:FindFirstChild("Handle") 
    if character and Humanoid and RootPart then
        FlingDied = Humanoid.Died:Connect(function()
            FlingLock = true
            OnOff4 = true
        end)
        if RootPart.Velocity.Magnitude < 500 then
            OldFling = RootPart.CFrame
        else
            OldFling_ = RootPart.CFrame
        end
        if THead then
            workspace.CurrentCamera.CameraSubject = THead
        elseif Handle then
            workspace.CurrentCamera.CameraSubject = Handle
        elseif THumanoid then
            workspace.CurrentCamera.CameraSubject = THumanoid
        elseif TRootPart then
            workspace.CurrentCamera.CameraSubject = TRootPart
        end
        local function FlingPosition(BasePart, Pos, Ang)
            RootPart.CFrame = CFrame.new(BasePart.Position) * Pos * Ang
            RootPart.Velocity = Vector3.new(9e7, 9e7 * 10, 9e7)
            RootPart.RotVelocity = Vector3.new(9e8, 9e8, 9e8)
        end
        local function FlingBasePart(BasePart)
            local FlingTimer = tick()
            local FlingDelay = 6
            local FlingAngle = 0
            FlingVel = Instance.new("BodyVelocity")
            FlingVel.Name = randomString()
            FlingVel.Velocity = Vector3.new(9e8, 9e8, 9e8)
            FlingVel.MaxForce = Vector3.new(1/0, 1/0, 1/0)
            FlingVel.Parent = RootPart
            repeat RunService.Heartbeat:Wait()
                if RootPart and Humanoid and TCharacter and THumanoid and TRootPart and BasePart then
                    execCmd('nosit')
                    if BasePart.Velocity.Magnitude < 50 then
                        FlingAngle = FlingAngle + 100
                        FlingPosition(BasePart, CFrame.new(0, 1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(FlingAngle), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, -1.5, 0) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(FlingAngle), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(2.25, 1.5, -2.25) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(FlingAngle), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(-2.25, -1.5, 2.25) + THumanoid.MoveDirection * BasePart.Velocity.Magnitude / 1.25, CFrame.Angles(math.rad(FlingAngle), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, 1.5, 0) + THumanoid.MoveDirection,CFrame.Angles(math.rad(FlingAngle), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, -1.5, 0) + THumanoid.MoveDirection,CFrame.Angles(math.rad(FlingAngle), 0, 0))
                        task.wait()
                    else
                        FlingPosition(BasePart, CFrame.new(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, -1.5, -THumanoid.WalkSpeed), CFrame.Angles(0, 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, 1.5, THumanoid.WalkSpeed), CFrame.Angles(math.rad(90), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, 1.5, TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(90), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, -1.5, -TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(0, 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, 1.5, TRootPart.Velocity.Magnitude / 1.25), CFrame.Angles(math.rad(90), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(math.rad(90), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(0, 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, -1.5 ,0), CFrame.Angles(math.rad(-90), 0, 0))
                        task.wait()
                        FlingPosition(BasePart, CFrame.new(0, -1.5, 0), CFrame.Angles(0, 0, 0))
                        task.wait()
                    end
                else
                    break
                end
            until not SkidFling_ or TextBox5.Text:match("^%s*$") or not (RootPart and Humanoid and TCharacter and THumanoid and TRootPart and BasePart) or not vFling:IsDescendantOf(Players) or vFling.Character ~= TCharacter or not BasePart:IsDescendantOf(vFling.Character) or Humanoid.Health <= 0 or THumanoid.Health <= 0 or Humanoid.Health <= Humanoid.MaxHealth * 0.3 or BasePart.Velocity.Magnitude > 500 or tick() > FlingTimer + FlingDelay
        end
        if TRootPart and THead then
            if (TRootPart.Position - THead.Position).Magnitude > 5 then
                FlingBasePart(THead)
            else
                FlingBasePart(TRootPart)
            end
        elseif TRootPart and not THead then
            FlingBasePart(TRootPart)
        elseif not TRootPart and THead then
            FlingBasePart(THead)
        elseif not TRootPart and not THead and Accessory and Handle then
            FlingBasePart(Handle)
        end
        if Humanoid then
            workspace.CurrentCamera.CameraSubject = Humanoid
        end
        if RootPart and (OldFling or OldFling_) then
            repeat RunService.Heartbeat:Wait()
                execCmd('breakvelocity')
                RootPart.CFrame = (OldFling or OldFling_) * CFrame.new(0, 3, 0)
            until (RootPart.Position - (OldFling or OldFling_).Position).Magnitude < 25
        end
    end
    if FlingVel then
        FlingVel:Destroy()
        FlingVel = nil
    end
    SkidFling_ = false
    task.wait(1)
    if FlingDied then
        FlingDied:Disconnect()
        FlingDied = nil
    end
    Frame1.BackgroundColor3 = Black
    execCmd('unnosit')
    Lock3 = false
end

TextButton7.Activated:Connect(function()
    local players = getPlayer(TextBox5.Text, speaker)
    for _, v in ipairs(players) do
        local vFling = Players:FindFirstChild(v)
        if vFling and vFling ~= speaker and vFling.Character and vFling.Character:FindFirstChildWhichIsA("BasePart") then
            SkidFling(vFling)
        end
    end
end)

local LoopBring = nil
local BringTimer = 0
local BringDelay = 0.1

TextButton5.Activated:Connect(function()
    if Lock2 then
        return
    end
    if TextBox3.Text:match("^%s*$") or TextBox3.Text == "me" then
        return
    end
    local players_ = getPlayer(TextBox3.Text, speaker)
    if #players_ == 0 then
        return
    end
    OnOff3 = not OnOff3
    TextButton5.Text = OnOff3 and "Loop Bring / On" or "Loop Bring / Off"
    if OnOff3 then
        OnDied()
        LoopBring = RunService.Heartbeat:Connect(function(dt)
            BringTimer += dt
            if BringTimer < BringDelay then 
                return 
            end
            BringTimer = 0
            execCmd('nosit')
            local players = {}
            if string.match(TextBox3.Text, "^#(%d+)$") then
                for _, a in ipairs(players_) do
                    if a ~= speaker.Name then
                        table.insert(players, a)
                    end
                end
            elseif TextBox3.Text == "random" then
                for _, b in ipairs(players_) do
                    if b ~= speaker.Name then
                        table.insert(players, b)
                    end
                end
            else
                for _, c in ipairs(getPlayer(TextBox3.Text, speaker)) do
                    if c ~= speaker.Name then
                        table.insert(players, c)
                    end
                end
            end
            for i = #players, 1, -1 do
                if not Players:FindFirstChild(players[i]) then
                    table.remove(players, i)
                end
            end
            if TextBox3.Text:match("^%s*$") or #players == 0 or myHumanoid and myHumanoid.Health <= myHumanoid.MaxHealth * 0.3 then
                if LoopBring then
                    LoopBring:Disconnect()
                    LoopBring = nil
                end
                Lock2 = true
                task.wait(1)
                TextButton5.Text = "Loop Bring / Off"
                execCmd('unnosit')
                Lock2 = false
                return
            end
            local N = 3
            if TextBox4.Text and isNumber(TextBox4.Text) then
                N = tonumber(TextBox4.Text)
            end
            for _, v in ipairs(players) do
                local vBring = Players:FindFirstChild(v)
                if vBring and vBring.Character then
                    local Target = getRoot(vBring.Character)
                    local Player = getRoot(speaker.Character)
                    if Target and Player then
                        Target.CFrame = Player.CFrame * CFrame.new(0, 0, -N)
                    end
                end
            end
        end)
    else
        if LoopBring then
            LoopBring:Disconnect()
            LoopBring = nil
        end
        Lock2 = true
        task.wait(1)
        execCmd('unnosit')
        Lock2 = false
    end
end)

TextButton3.Activated:Connect(function()
    OnOff2 = not OnOff2
    TextButton3.Text = OnOff2 and "Teleport Tool / On" or "Teleport Tool / Off"
    if OnOff2 then
        local TpTool = Instance.new("Tool")
	    TpTool.Name = "Teleport Tool (a)"
	    TpTool.RequiresHandle = false
	    TpTool.Parent = speaker.Backpack
	    TpTool.Activated:Connect(function()
		local Char = speaker.Character or workspace:FindFirstChild(speaker.Name)
		local HRP = Char and Char:FindFirstChild("HumanoidRootPart")
		if not Char or not HRP then
			return 
        end
		HRP.CFrame = CFrame.new(IYMouse.Hit.X, IYMouse.Hit.Y + 3, IYMouse.Hit.Z, select(4, HRP.CFrame:components()))
	    end)
        task.wait(0.1)
        local Equip = speaker.Character:FindFirstChildWhichIsA("Humanoid")
        if Equip then
            Equip:EquipTool(TpTool)
        end
    else
        local Unequip = speaker.Backpack:FindFirstChild("Teleport Tool (a)") or speaker.Character:FindFirstChild("Teleport Tool (a)")
        if Unequip then
            Unequip:Destroy()
        end
	end
end)

local LoopGoto = nil
local OldGoto = nil
local OldGoto_ = nil
local GotoTimer = 0
local GotoDelay = 0.1
local GotoIndex = 1

TextButton2.Activated:Connect(function()
    if Lock1 then
        return
    end
    if TextBox2.Text:match("^%s*$") or TextBox2.Text == "me" then
        return
    end
    local players_ = getPlayer(TextBox2.Text, speaker)
    if #players_ == 0 then
        return
    end
    OnOff1 = not OnOff1
    TextButton2.Text = OnOff1 and "Loop Goto / On" or "Loop Goto / Off"
    if OnOff1 then
        OnDied()
        if myRootPart then
            if myRootPart.Velocity.Magnitude < 500 then
                OldGoto = myRootPart.CFrame
            else
                OldGoto_ = myRootPart.CFrame
            end
        end
        GotoIndex = 1
        LoopGoto = RunService.Heartbeat:Connect(function(dt)
            GotoTimer += dt
            if GotoTimer < GotoDelay then 
                return 
            end
            GotoTimer = 0
            execCmd('nosit')
            local players = {}
            if string.match(TextBox2.Text, "^#(%d+)$") then
                for _, a in ipairs(players_) do
                    if a ~= speaker.Name then
                        table.insert(players, a)
                    end
                end
            elseif TextBox2.Text == "random" then
                for _, b in ipairs(players_) do
                    if b ~= speaker.Name then
                        table.insert(players, b)
                    end
                end
            else
                for _, c in ipairs(getPlayer(TextBox2.Text, speaker)) do
                    if c ~= speaker.Name then
                        table.insert(players, c)
                    end
                end
            end
            for i = #players, 1, -1 do
                if not Players:FindFirstChild(players[i]) then
                    table.remove(players, i)
                end
            end
            if TextBox2.Text:match("^%s*$") or #players == 0 or myHumanoid and myHumanoid.Health <= myHumanoid.MaxHealth * 0.3 then
                if LoopGoto then
                    LoopGoto:Disconnect()
                    LoopGoto = nil
                end
                if myRootPart and (OldGoto or OldGoto_) then
                    myRootPart.CFrame = (OldGoto or OldGoto_) * CFrame.new(0, 3, 0)
                    execCmd('breakvelocity')
                end
                Lock1 = true
                task.wait(1)
                TextButton2.Text = "Loop Goto / Off"
                execCmd('unnosit')
                Lock1 = false
                return
            end
            execCmd('breakvelocity')
            if GotoIndex > #players then
                GotoIndex = 1
            end
            local vGoto = Players:FindFirstChild(players[GotoIndex])
            if vGoto and vGoto.Character then
                local Player = getRoot(speaker.Character)
                local Target = getRoot(vGoto.Character)
                if Player and Target then
                    Player.CFrame = Target.CFrame * CFrame.new(0, 0, 3)
                end
            end
            GotoIndex = GotoIndex + 1
            if GotoIndex > #players then
                GotoIndex = 1
            end
        end)
    else
        if LoopGoto then
            LoopGoto:Disconnect()
            LoopGoto = nil
        end
        if myRootPart and (OldGoto or OldGoto_) then
            myRootPart.CFrame = (OldGoto or OldGoto_) * CFrame.new(0, 3, 0)
            execCmd('breakvelocity')
        end
        Lock1 = true
        task.wait(1)
        execCmd('unnosit')
        Lock1 = false
    end
end)

TextBox1.FocusLost:Connect(function()
    if TextBox1.Text:match("^%s*$") or TextBox1.Text == "me" then
        return
    end
    local players_ = getPlayer(TextBox1.Text, speaker)
    if #players_ == 0 then
        return
    end
	local players = getPlayer(TextBox1.Text, speaker)
	for i,v in pairs(players)do
		if Players[v].Character ~= nil then
			getRoot(speaker.Character).CFrame = getRoot(Players[v].Character).CFrame * CFrame.new(0, 0, 3)
		end
	end
	execCmd('breakvelocity')
end)

speaker.CharacterAdded:Connect(function()
	repeat task.wait(1) until myRootPart
    if OnOff15 then
        execCmd('unvehiclefly')
        execCmd('vehiclefly ' .. SetStats[StatsMode].Fly)
    end
    if not OnOff14 then
        OldStats()
    end
    if OnOff11 then
        TextButton15.TextColor3 = White
        if LoopSt then
            LoopSt:Disconnect()
            LoopSt = nil
        end
        execCmd("unstrengthen")
        OnOff11 = false
    end
    if OnOff10 then
        TextButton14.TextColor3 = White
        execCmd('unfreeze')
        if LoopFr then
            LoopFr:Disconnect()
            LoopFr = nil
        end
        execCmd('unfreezeanims')
        execCmd('unnosit')
        OnOff10 = false
    end
    if OnOff9 then
        TextButton13.TextColor3 = White
        execCmd('vehicleclip')
        OnOff9 = false
    end
    if OnOff7 then
        Frame3.BackgroundColor3 = Black
        if workspace and workspace:FindFirstChild("InvisChair") then
            workspace.InvisChair:Destroy()
        end
        if workspace and InvisVoid then
            workspace.FallenPartsDestroyHeight = InvisVoid
        end
        if LoopInvis then
            LoopInvis:Disconnect()
            LoopInvis = nil
        end
        InvisTransparency(0)
        execCmd('unnosit')
        OnOff7 = false
    end
    if OnOff6 then
        TextButton9.Text = "Touch Fling / Off"
        TouchFling = false
        OnOff6 = false
    end
    if OnOff4 then
        if myRootPart and (OldFling or OldFling_) then
            myRootPart.CFrame = (OldFling or OldFling_) * CFrame.new(0, 3, 0)
        end
        execCmd('breakvelocity')
        FlingLock = false
        OnOff4 = false
    end
    if OnOff3 then
        TextButton5.Text = "Loop Bring / Off"
        if LoopBring then
            LoopBring:Disconnect()
            LoopBring = nil
        end
        execCmd('unnosit')
        OnOff3 = false
    end
    if OnOff2 then
        TextButton3.Text = "Teleport Tool / Off"
        OnOff2 = false
    end
    if OnOff1 then
        TextButton2.Text = "Loop Goto / Off"
        if myRootPart and (OldGoto or OldGoto_) then
            myRootPart.CFrame = (OldGoto or OldGoto_) * CFrame.new(0, 3, 0)
        end
        if LoopGoto then
            LoopGoto:Disconnect()
            LoopGoto = nil
        end
        execCmd('breakvelocity')
        execCmd('unnosit')
        OnOff1 = false
    end
end)

local Died = nil

function OnDied()
    if myHumanoid then 
        if Died then
            Died:Disconnect()
            Died = nil
        end
        Died = myHumanoid.Died:Connect(function()
            if OnOff11 then
                if LoopSt then
                    LoopSt:Disconnect()
                    LoopSt = nil
                end
                execCmd("unstrengthen")
            end
            if OnOff10 then
                execCmd('unfreeze')
                if LoopFr then
                    LoopFr:Disconnect()
                    LoopFr = nil
                end
                execCmd('unfreezeanims')
            end
            if OnOff9 then
                execCmd('unnoclip nonotify')
            end
            if OnOff7 then
                if LoopInvis then
                    LoopInvis:Disconnect()
                    LoopInvis = nil
                end
            end
            if OnOff6 then
                TouchFling = false
            end
            if OnOff3 then
                if LoopBring then
                    LoopBring:Disconnect()
                    LoopBring = nil
                end
            end
            if OnOff1 then
                if LoopGoto then
                    LoopGoto:Disconnect()
                    LoopGoto = nil
                end
            end
        end)
    end
end

local SpawnLock = false
local SpawnMode = false

SpawnPoint.Activated:Connect(function()
    if SpawnLock then
        return
    end
    SpawnMode = not SpawnMode
    SpawnPoint.TextColor3 = SpawnMode and Cyan or White
    if SpawnMode then
        execCmd('spawnpoint 0.3')
    else
        execCmd('nospawnpoint')
        SpawnLock = true
        task.wait(1)
        SpawnLock = false
    end
end)

local FlashLock = false
local FlashMode = false
local flashback = nil

Flashback.Activated:Connect(function()
    if FlashLock then
        return
    end
    FlashMode = not FlashMode
    Flashback.TextColor3 = FlashMode and Green or White
    if FlashMode then
        if flashback then
            flashback:Disconnect()
            flashback = nil
        end
        flashback = speaker.CharacterAdded:Connect(function()
            repeat task.wait(0.6) until myRootPart
            execCmd('flashback')
        end)
    else
        if flashback then
            flashback:Disconnect()
            flashback = nil
        end
        FlashLock = true
        task.wait(1)
        FlashLock = false
    end
end)

local PointLock = false
local PointMode = false
local checkpoint = nil

Checkpoint.Activated:Connect(function()
    if PointLock then
        return
    end
    PointMode = not PointMode
    Checkpoint.TextColor3 = PointMode and Red or White
    if PointMode then
        local Pos = getRoot(speaker.Character).CFrame
        if myRootPart then
            checkpoint = myRootPart.CFrame
        end
        notify('Checkpoint','Checkpoint saved at '..tostring(Pos))
    else
        if myRootPart and checkpoint then
            myRootPart.CFrame = checkpoint * CFrame.new(0, 3, 0)
            execCmd('breakvelocity')
        end
        notify('Checkpoint','Back to checkpoint')
        OldPoint = nil
        PointLock = true
        task.wait(1)
        PointLock = false
    end
end)

local InfLock = false
local InfMode = false
local InfLoop = nil
local OldZoom = nil
local OldVoid = nil
 
InfiniteLoop.Activated:Connect(function()
    if InfLock then
        return
    end
    InfMode = not InfMode
    InfiniteLoop.TextColor3 = InfMode and Purple or White 
    if InfMode then
        execCmd('infinitejump')
        OldZoom = speaker and speaker.CameraMaxZoomDistance or 100
        OldVoid = workspace and workspace.FallenPartsDestroyHeight or -500
        InfLoop = RunService.Stepped:Connect(function()
            speaker.CameraMaxZoomDistance = 1/0
            workspace.FallenPartsDestroyHeight = 0/0
        end)
    else
        if InfLoop then
            InfLoop:Disconnect()
            InfLoop = nil
        end
        if speaker and OldZoom then
            speaker.CameraMaxZoomDistance = OldZoom
        end
        if workspace and OldVoid then
            workspace.FallenPartsDestroyHeight = OldVoid
        end
        execCmd('uninfinitejump')
        InfLock = true
        task.wait(1)
        InfLock = false
    end
end)

local FPSLock = false
local FPSMode = 0
local FPS = false

local SetFPS = {
    [1] = function()
        FPS = true
        SetFPSCap.TextColor3 = Red
        execCmd('setfpscap 15')
        execCmd('datalimit 400')
    end,
    [2] = function() 
        SetFPSCap.TextColor3 = Yellow
        execCmd('setfpscap 30')
        execCmd('datalimit 800')
    end,
    [3] = function()
        SetFPSCap.TextColor3 = Green
        execCmd('setfpscap 120')
        execCmd('datalimit 2500')
    end,
    [4] = function()
        FPS = false
        SetFPSCap.TextColor3 = White
        execCmd('setfpscap 60')
        execCmd('datalimit 1500')
    end
}

SetFPSCap.Activated:Connect(function()
    if FPSLock then 
        return
    end
    FPSLock = true
    FPSMode = FPSMode % #SetFPS + 1
    SetFPS[FPSMode]()
    task.delay(1, function()
        FPSLock = false
    end)
end)

local FixingLock = false
local FixingTouch = 0
local FixingDelay = 0.2

FixErrors.Activated:Connect(function()
    if FixingLock then
        return
    end
	local FixingTimer = tick()
    if FixingTimer - FixingTouch <= FixingDelay then
        FixErrors.TextColor3 = Red
        FixingLock = true
        Lock14 = false
        Lock13 = false
        Lock10 = false
        Lock9 = false
        Lock8 = false
        Lock7 = false
        Lock6 = false
        Lock5 = false
    	Lock4 = false
		Lock3 = false
		Lock2 = false
		Lock1 = false
        OnOff15 = false
        OnOff14 = false
        OnOff11 = false
        OnOff10 = false
        OnOff9 = false
        OnOff8 = false
        OnOff7 = false
        OnOff6 = false
		OnOff5 = false
		OnOff4 = false
		OnOff3 = false
		OnOff2 = false
		OnOff1 = false
        Frame4.BackgroundColor3 = Black
        Frame3.BackgroundColor3 = Black
        Frame2.BackgroundColor3 = Black
        Frame1.BackgroundColor3 = Black
        TextButton22.TextColor3 = White
        TextButton21.TextColor3 = White
        TextButton20.TextColor3 = White
        TextButton15.TextColor3 = White
        TextButton14.TextColor3 = White
        TextButton13.TextColor3 = White
        TextButton9.Text = "Touch Fling / Off"
        TextButton5.Text = "Loop Bring / Off"
        TextButton2.Text = "Loop Goto / Off"
        StatsMode = 0
        if LoopSt then
            LoopSt:Disconnect()
            LoopSt = nil
        end
        if LoopFr then
            LoopFr:Disconnect()
            LoopFr = nil
        end
        ESPMode = false
    	for i, c in pairs(COREGUI:GetChildren()) do
		    if string.sub(c.Name, -4) == '_ESP' then
			    c:Destroy()
		    end
    	end
        if workspace and workspace:FindFirstChild("InvisChair") then
            workspace.InvisChair:Destroy()
        end
        if workspace and InvisVoid then
            workspace.FallenPartsDestroyHeight = InvisVoid
        end
        if LoopInvis then
            LoopInvis:Disconnect()
            LoopInvis = nil
        end
        InvisTransparency(0)
        TouchFling = false
        if AntiFling then
            AntiFling:Disconnect()
            AntiFling = nil
        end
        SkidFling_ = false
        OldFling = nil
        OldFling_ = nil
        if FlingDied then
            FlingDied:Disconnect()
            FlingDied = nil
        end
        if FlingVel then
            FlingVel:Destroy()
            FlingVel = nil
        end
        if LoopBring then
            LoopBring:Disconnect()
            LoopBring = nil
        end
        OldGoto = nil
        OldGoto_ = nil
        if LoopGoto then
            LoopGoto:Disconnect()
            LoopGoto = nil
        end
        if Died then
            Died:Disconnect()
            Died = nil
        end
        execCmd('unfreeze')
        execCmd('unfreezeanims')
        execCmd("unstrengthen")
        execCmd('unnoclip')
        execCmd('vehicleclip')
        execCmd('unvehiclefly')
        execCmd('unloopspeed')
        execCmd('unloopjumppower')
        execCmd('unantifling')
        execCmd('unnosit')
        execCmd('breakvelocity')
        task.wait(0.1)
        execCmd('walkspeed ' .. OldSpeed)
        execCmd('jumppower ' .. OldJump)
        execCmd('gravity ' .. OldGrav)
        execCmd('reset')
        execCmd('respawn')
        task.wait(1)
        FixErrors.TextColor3 = White
        FixingLock = false
        FixingTouch = 0
    else
        FixingTouch = FixingTimer
    end
end)

Players.PlayerAdded:Connect(function(plr)
	if ESPMode then
		repeat wait(1) until plr.Character and getRoot(plr.Character)
		ESP(plr)
	end
end)

Players.PlayerRemoving:Connect(function(player)
    if ESPMode then
		for i, v in pairs(COREGUI:GetChildren()) do
			if v.Name == player.Name..'_ESP' then
				v:Destroy()
			end
		end
	end
    if player == speaker and FPS then
        execCmd('setfpscap 60')
        execCmd('datalimit 1500')
    end
end)

task.wait(1)

execCmd('darkchat')
execCmd('setfpscap 60')
execCmd('datalimit 1500')
execCmd('maxslopeangle')