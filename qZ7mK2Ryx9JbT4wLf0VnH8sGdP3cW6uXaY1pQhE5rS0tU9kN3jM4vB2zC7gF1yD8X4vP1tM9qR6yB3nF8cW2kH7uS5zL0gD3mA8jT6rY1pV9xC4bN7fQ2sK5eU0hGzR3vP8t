        TextButton3.Visible = false
        TextButton2.Visible = false
        FixErrors.Position = UDim2.new(0.5, -25, 0.5, 56)
        InfiniteLoop.Position = UDim2.new(0.5, -60, 0.5, 56)
        SetFPSCap.Position = UDim2.new(0.5, -95, 0.5, 56)
    end,
    [2] = function()
        FixErrors.Visible = false
        InfiniteLoop.Visible = false
        SetFPSCap.Visible = false
    end,
    [3] = function()
        FixErrors.Position = UDim2.new(0.5, -130, 0.5, -34)
        InfiniteLoop.Position = UDim2.new(0.5, -130, 0.5, -4) 
        SetFPSCap.Position = UDim2.new(0.5, -130, 0.5, 26)
        TextBox2.Visible = true
        TextBox1.Visible = true
        TextButton3.Visible = true
        TextButton2.Visible = true
        SetFPSCap.Visible = true
        InfiniteLoop.Visible = true
        FixErrors.Visible = true
    end
}

TextButton1.Activated:Connect(function()
    SetGui1 = SetGui1 % #Section1 + 1
    Section1[SetGui1]()
end)

local StatsLock = false
local StatsMode = 0

local SetStats = {
    [0] = { 
        Color = White, Fly = 3, WS = 75, JPR15 = 109, JPR6 = 99, JHR15 = 66.5, JHR6 = 65.5, Grav = 255.2
    },
    [1] = { 
        Color = Red, Fly = 10, WS = 150, JPR15 = 184, JPR6 = 174, JHR15 = 141.5, JHR6 = 140.5, Grav = 330.2
    },
    [2] = { 
        Color = Purple, Fly = 20, WS = 300, JPR15 = 334, JPR6 = 324, JHR15 = 291.5, JHR6 = 290.5, Grav = 480.2
    },
    [3] = { 
        Color = Blue, Fly = 30, WS = 600, JPR15 = 634, JPR6 = 624, JHR15 = 591.5, JHR6 = 590.5, Grav = 780.2
    },
}

local OldSpeed = nil
local OldJump = nil
local OldGrav = nil

local function OldStats()
    if workspace and myHumanoid then
        if myHumanoid.WalkSpeed and myHumanoid.WalkSpeed > 0 then
            OldSpeed = myHumanoid.WalkSpeed
        else
            OldSpeed = 16
        end
        if myHumanoid.UseJumpPower then
            if myHumanoid.JumpPower and myHumanoid.JumpPower > 0 then
                OldJump = myHumanoid.JumpPower
            else
                OldJump = r15(speaker) and 50 or 40
            end
        else
            if myHumanoid.JumpHeight and myHumanoid.JumpHeight > 0 then
                OldJump = myHumanoid.JumpHeight
            else
                OldJump = r15(speaker) and 7.5 or 6.5
            end
        end
        if workspace.Gravity and workspace.Gravity > 0 then
            OldGrav = workspace.Gravity
        else
            OldGrav = 196.2
        end
    else
        OldSpeed = 16
        OldJump = 50
        OldGrav = 196.2
    end
end

OldStats()

TextButton22.Activated:Connect(function()
    if Lock14 then
        return
    end
    OnOff15 = not OnOff15
    TextButton22.TextColor3 = OnOff15 and Green or White
    if OnOff15 then
        execCmd('vehiclefly ' .. SetStats[StatsMode].Fly)
    else
        execCmd('unvehiclefly')
        Lock14 = true
        task.wait(1)
        Lock14 = false
    end
end)

TextButton21.Activated:Connect(function()
    if StatsLock then 
        return
    end
    StatsLock = true
    StatsMode = (StatsMode + 1) % 4
    TextButton21.TextColor3 = SetStats[StatsMode].Color
    if OnOff15 then
        execCmd('unvehiclefly')
        execCmd('vehiclefly ' .. SetStats[StatsMode].Fly)
    end
    if OnOff14 then
        execCmd('unloopspeed')
        execCmd('unloopjumppower')
        execCmd('loopspeed ' .. SetStats[StatsMode].WS)
        if myHumanoid and myHumanoid.UseJumpPower then
            if r15(speaker) then
                execCmd('loopjumppower ' .. SetStats[StatsMode].JPR15)
            else
                execCmd('loopjumppower ' .. SetStats[StatsMode].JPR6)
            end
        else
            if r15(speaker) then
                execCmd('loopjumppower ' .. SetStats[StatsMode].JHR15)
            else
                execCmd('loopjumppower ' .. SetStats[StatsMode].JHR6)
            end
        end
        execCmd('gravity ' .. SetStats[StatsMode].Grav)
    end
    task.delay(1, function()
        StatsLock = false
    end)
end)

TextButton20.Activated:Connect(function()
    if Lock13 then 
        return
    end
    OnOff14 = not OnOff14
    TextButton20.TextColor3 = OnOff14 and Yellow or White
    if OnOff14 then
        execCmd('loopspeed ' .. SetStats[StatsMode].WS)
        if myHumanoid and myHumanoid.UseJumpPower then
            if r15(speaker) then
                execCmd('loopjumppower ' .. SetStats[StatsMode].JPR15)
            else
                execCmd('loopjumppower ' .. SetStats[StatsMode].JPR6)
            end
        else
            if r15(speaker) then
                execCmd('loopjumppower ' .. SetStats[StatsMode].JHR15)
            else
                execCmd('loopjumppower ' .. SetStats[StatsMode].JHR6)
            end
        end
        execCmd('gravity ' .. SetStats[StatsMode].Grav)
    else
        execCmd('unloopspeed')
        execCmd('unloopjumppower')
        task.wait(0.1)
        execCmd('walkspeed ' .. OldSpeed)
        execCmd('jumppower ' .. OldJump)
        execCmd('gravity ' .. OldGrav)
        Lock13 = true
        task.wait(1)
        Lock13 = false
    end
end)

local LoopCam = nil

TextButton18.Activated:Connect(function()
    if Lock12 then
        return
    end
    OnOff13 = not OnOff13
    TextButton18.TextColor3 = OnOff13 and Azure or White
    if OnOff13 then
        if LoopCam then
            LoopCam:Disconnect()
            LoopCam = nil
        end
        LoopCam = RunService.RenderStepped:Connect(function()
            if workspace and myRootPart and myHumanoid and not myHumanoid.SeatPart then
                myHumanoid.AutoRotate = false
                local Camera = workspace.CurrentCamera.CFrame.LookVector
		    	local Rotate = CFrame.lookAt(myRootPart.Position, myRootPart.Position + Vector3.new(Camera.X, 0, Camera.Z))
			    myRootPart.CFrame = Rotate
            end
        end)
    else
        if myHumanoid then
            myHumanoid.AutoRotate = true
        end
        if LoopCam then
            LoopCam:Disconnect()
            LoopCam = nil
        end
        Lock12 = true
        task.wait(1)
        Lock12 = false
    end
end)

local JumpMode = false
local LoopJump = nil
local SafeJump = nil

local HideJump = RunService.RenderStepped:Connect(function()
	local PlayerGui = speaker:FindFirstChild("PlayerGui")
    local JumpGui = PlayerGui and PlayerGui:FindFirstChild("JumpButton", true)
    if JumpGui then
        JumpGui.Visible = SetGui5 == 1
    end
end)

local function TouchJump(touchPos)
	local absPos = TextButton19.AbsolutePosition
	local absSize = TextButton19.AbsoluteSize
	return (
		touchPos.X >= absPos.X and touchPos.X <= absPos.X + absSize.X and
		touchPos.Y >= absPos.Y and touchPos.Y <= absPos.Y + absSize.Y
	)
end

local function ActivateJump()
	if not JumpMode then
		JumpMode = true
        if myHumanoid and myHumanoid:IsDescendantOf(myCharacter) then
	        execCmd("jump")
        end
		LoopJump = RunService.Heartbeat:Connect(function()
			if JumpMode and myHumanoid and myHumanoid:IsDescendantOf(myCharacter) then
				execCmd("jump")
			end
		end)
	end
end

local function DeactivateJump()
	JumpMode = false
	if LoopJump then
		LoopJump:Disconnect()
		LoopJump = nil
	end
	
end

UserInputService.TouchStarted:Connect(function(touch)
	if not SafeJump and TouchJump(touch.Position) then
		SafeJump = touch
		ActivateJump()
	end
end)

UserInputService.TouchMoved:Connect(function(touch)
	if SafeJump and touch == SafeJump then
		if not TouchJump(touch.Position) then
			DeactivateJump()
			SafeJump = nil
		end
	end
end)

UserInputService.TouchEnded:Connect(function(touch)
	if SafeJump and touch == SafeJump then
		DeactivateJump()
		SafeJump = nil
	end
end)

